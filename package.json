    "dev:server": "set NODE_ENV=development && tsx server/index.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outfile=dist/server.js",
    "build:dev": "vite build --mode development",
    "start": "set NODE_ENV=production && node dist/server.js",
    "check": "tsc",
    "check-imports": "node ./scripts/check-imports.js",
    "db:push": "drizzle-kit push",
    "lovable:prepare": "echo 'Preparando ambiente para desenvolvimento' && npm install",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "format:editor": "prettier --write \"src/components/editor/**/*.{ts,tsx}\"",
    "format:context": "prettier --write \"src/context/**/*.{ts,tsx}\"",
    "format:hooks": "prettier --write \"src/hooks/**/*.{ts,tsx}\"",
    "templates:standardize": "node scripts/standardize-templates.js",
    "templates:convert": "node scripts/convert-to-modular.js",
    "templates:cleanup": "node scripts/template-cleanup.js",
    "templates:format": "prettier --write \"src/components/steps/**/*.{ts,tsx}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage"
